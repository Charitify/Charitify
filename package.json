{
  "name": "charitify",
  "description": "Charity app for helping those in need.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ENV_FILE_PATH=./environments/.env sapper dev",
    "prod": "ENV_FILE_PATH=./environments/.env.prod sapper dev",
    "build": "ENV_FILE_PATH=./environments/.env.prod sapper build --legacy",
    "export": "ENV_FILE_PATH=./environments/.env.prod sapper export --legacy --basepath Charitify",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "deploy": "npm run export && gh-pages -d __sapper__/export/Charitify && git add . && git commit -m 'deploy' && git push origin develop"
  },
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "body-parser": "^1.19.0",
    "body-scroll-lock": "^3.0.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.1",
    "dayjs": "^1.8.30",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "hammerjs": "^2.0.8",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.1",
    "js-storage": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "lodash-es": "^4.17.15",
    "moment": "^2.29.4",
    "mongoose": "^5.9.25",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.0",
    "sirv": "^0.4.6",
    "svelte-icons": "^2.1.0",
    "svelte-persistent-store": "^0.1.3",
    "winston": "3.3.3",
    "zl-fetch": "^3.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "^6.1.0",
    "@rollup/plugin-replace": "^2.3.3",
    "autoprefixer": "^9.8.5",
    "babel-plugin-lodash": "^3.3.4",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-visualizer": "^3.3.2",
    "sapper": "^0.27.16",
    "sapper-environment": "^1.0.1",
    "svelte": "^3.24.0",
    "svelte-preprocess": "^3.9.12"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Bublikus/Charitify.git"
  },
  "license": "SEE LICENSE IN README.md"
}
